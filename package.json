{
  "name": "jerrycan_basic_template",
  "version": "1.0.0",
  "description": "basic react/redux template using jerrycan",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start"
  },
  "betterScripts": {
    "start": {
      "command": "jerrycan && webpack-dev-server --config ./webpack.config.babel.js",
      "env": {
        "NODE_ENV": "development",
        "MODULES": "[\"module_one\", \"module_two\", \"module_three\"]",
        "CONFIG_TYPE": "rc",
        "DEFAULTE_ROUTE": "/"
      }
    }
  },
